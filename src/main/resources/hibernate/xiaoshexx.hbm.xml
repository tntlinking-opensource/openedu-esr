		<many-to-one name="syfxthree" class="org.pfw.framework.ywmk.domain.DictThree">
			<column name="syfxthree" length="32">
				<comment>使用方向(三级)</comment>
			</column>
		</many-to-one>
		<property name="xsmj" type="java.lang.String">
			<column name="xsmj" length="10">
				<comment>校舍面积</comment>
			</column>
