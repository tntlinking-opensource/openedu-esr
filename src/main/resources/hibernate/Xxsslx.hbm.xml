<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.pfw.framework.ywmk.domain.Xxsslx" table="t_ywmk_xxsslx">
		<comment>学校所属类型</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<many-to-one name="ssxx" class="org.pfw.framework.ywmk.domain.Xxxx">
			<column name="ssxx" length="32">
				<comment>所属学校</comment>
			</column>
		</many-to-one>
		<many-to-one name="xxlxid" class="org.pfw.framework.xtgl.domain.Dict">
			<column name="xxlxid" length="32">
				<comment>学校类型id</comment>
			</column>
		</many-to-one>
	</class>
</hibernate-mapping>