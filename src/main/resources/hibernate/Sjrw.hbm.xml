<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.pfw.framework.ywmk.domain.Sjrw" table="t_ywmk_Sjrw">
		<comment>报表取值配置</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id> 
		<property name="rwmc" type="java.lang.String">
			<column name="rwmc" length="100">
				<comment>任务名称</comment>
			</column>
		</property>
		<property name="nianf" type="java.lang.String">
			<column name="nianf" length="50">
				<comment>年份</comment>
			</column>
		</property>
	 
	</class>
</hibernate-mapping>