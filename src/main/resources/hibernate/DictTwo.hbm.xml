<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.pfw.framework.ywmk.domain.DictTwo" table="t_ywmk_DictTwo">
		<comment>数据数据字典</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<many-to-one name="sszd" class="org.pfw.framework.xtgl.domain.Dict">
			<column name="sszd" length="32">
				<comment>所属字典(二级)</comment>
			</column>
		</many-to-one>
		<property name="dm" type="java.lang.String">
			<column name="dm" length="50">
				<comment>代码</comment>
			</column>
		</property>
		<property name="name" type="java.lang.String">
			<column name="name" length="50">
				<comment>名称</comment>
			</column>
		</property>
		<property name="sfyx" type="java.lang.String">
			<column name="sfyx" length="1">
				<comment>是否有效(1:是,0:否)</comment>
			</column>
		</property>
		<property name="remark" type="java.lang.String">
			<column name="remark" length="200">
				<comment>备注</comment>
			</column>
		</property>
	</class>
</hibernate-mapping>