<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="org.pfw.framework.ywmk.domain.ReportInfo" table="T_Config_ReportInfo">
        <comment>报表信息</comment>
        
        <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        
        <property name="versionh" type="java.lang.String">
            <column name="versionh" length="50">
                <comment>版本号</comment>
            </column>
        </property>
        
        <property name="dm" type="java.lang.String">
            <column name="dm" length="50">
                <comment>报表代码</comment>
            </column>
        </property>
        
        <property name="reportcode" type="java.lang.String">
            <column name="reportCode" length="50">
                <comment>表号</comment>
            </column>
        </property>
        
        <property name="reportname" type="java.lang.String">
            <column name="reportName" length="100">
                <comment>报表名称</comment>
            </column>
        </property>
        
        <property name="reportlx" type="java.lang.String">
            <column name="reportLx" length="1">
                <comment>报表类型,1年报 2季报</comment>
            </column>
        </property>
        
        
        <property name="bbgs" type="java.lang.String">
            <column name="bbgs" length="1">
                <comment>报表归属  1 国家   2 教育局</comment>
            </column>
        </property>
         
        <property name="sequenceNumber" type="java.lang.Integer">
            <column name="sequenceNumber" length="10" >
            <comment>顺序号</comment>
            </column>
        </property>
        
        <property name="configytype" type="java.lang.String">
            <column name="configytype" length="1">
                <comment>配置类型: 1_配置,2_自定义url</comment>
            </column>
        </property>
        
        <property name="customURL" type="java.lang.String">
            <column name="customURL" length="500">
                <comment>url地址</comment>
            </column>
        </property>
        
        <property name="customURLDetails" type="java.lang.String">
            <column name="customURLDetails" length="500">
                <comment>url地址详情</comment>
            </column>
        </property>
        
        <property name="customURLShenh" type="java.lang.String">
            <column name="customURLShenh" length="500">
                <comment>url地址审核界面</comment>
            </column>
        </property>
        
        <property name="customURLzdzxcx" type="java.lang.String">
            <column name="customURLzdzxcx" length="500">
                <comment>url地址指导中心查询界面</comment>
            </column>
        </property>
        
        <property name="rowNumber" type="java.lang.Integer">
            <column name="rowNumber" length="2">
                <comment>行数</comment>
            </column>
        </property>
        
        <property name="columnNumber" type="java.lang.Integer">
            <column name="columnNumber" length="2">
                <comment>列数</comment>
            </column>
        </property>
        
        <property name="explains" type="java.lang.String">
            <column name="explains" length="1000">
                <comment>指标解释</comment>
            </column>
        </property>
        
        <property name="reportDescribe" type="java.lang.String">
            <column name="reportDescribe" length="1000">
                <comment>说明</comment>
            </column>
        </property>
        
        <property name="verifyRelations" type="java.lang.String">
            <column name="verifyRelations" length="1000">
                <comment>校验关系</comment>
            </column>
        </property>
        
        <property name="syxxids" type="java.lang.String">
            <column name="syxxids" length="500">
                <comment>适用学校id(逗号隔开),学校类型取自字典</comment>
            </column>
        </property>
        
        <property name="syxxmcs" type="java.lang.String">
            <column name="syxxmcs" length="500">
                <comment>适用学校名称(用于界面显示),学校类型取自字典</comment>
            </column>
        </property>
        
        <property name="bz" type="java.lang.String">
            <column name="bz" length="100">
                <comment>备注</comment>
            </column>
        </property>
        
        <property name="reptTable" type="java.lang.String">
            <column name="reptTable" length="1000">
                <comment>表格html存储</comment>
            </column>
        </property>
        
        <property name="reptTableHead" type="java.lang.String">
            <column name="reptTableHead" length="1000">
                <comment>表格头html存储</comment>
            </column>
        </property>
        
        <property name="jldw" type="java.lang.String">
            <column name="jldw" length="20">
                <comment>计量单位</comment>
            </column>
        </property>
        
        <property name="jgbmc1" type="java.lang.String">
            <column name="jgbmc1" length="50">
                <comment>保存结果表1</comment>
            </column>
        </property>
        
        <property name="jgbmc2" type="java.lang.String">
            <column name="jgbmc2" length="50">
                <comment>保存结果表2</comment>
            </column>
        </property>
        
        <property name="sffxqtb" type="java.lang.String">
            <column name="sffxqtb" length="1">
                <comment>是否分校区填报  0否  1是</comment>
            </column>
        </property>
         
        <property name="sffzxy" type="java.lang.String">
            <column name="sffzxy" length="1">
                <comment>是否复制下移  0否  1是</comment>
            </column>
        </property>
        
        <property name="pzggsj" type="java.lang.String">
            <column name="pzggsj" length="30">
                <comment>配置更改时间</comment>
            </column>
        </property>
        
        <many-to-one name="pzgguser" class="org.pfw.framework.domain.security.User" fetch="select">
            <column name="pzgguser" length="32">
                <comment>配置更改修改人</comment>
            </column>
        </many-to-one>
        
        <property name="dcmbwjm" type="java.lang.String">
            <column name="dcmbwjm" length="200">
                <comment>导出模版文件名</comment>
            </column>
        </property>
        
        <property name="dcmbscsj" type="java.lang.String">
            <column name="dcmbscsj" length="30">
                <comment>导出模版生成时间</comment>
            </column>
        </property>
        
        <many-to-one name="dcmbscuser" class="org.pfw.framework.domain.security.User" fetch="select">
            <column name="dcmbscuser" length="32">
                <comment>导出模版生成操作人</comment>
            </column>
        </many-to-one>
        
        <property name="drmbwjm" type="java.lang.String">
            <column name="drmbwjm" length="200">
                <comment>导入模版文件名</comment>
            </column>
        </property>
        
        <property name="drmbscsj" type="java.lang.String">
            <column name="drmbscsj" length="30">
                <comment>导入模版生成时间</comment>
            </column>
        </property>
        
        <many-to-one name="drmbscuser" class="org.pfw.framework.domain.security.User" fetch="select">
            <column name="drmbscuser" length="32">
                <comment>导入模版生成操作人</comment>
            </column>
        </many-to-one>
        
		<set name="rowFilterSet">
            <key>
                <column name="reportId" length="32">
                    <comment>报表id</comment>
                </column>
            </key>
            <one-to-many class="org.pfw.framework.ywmk.domain.ReportInfoRowFilter" />
        </set>
        
        <property name="bbnf" type="java.lang.String">
            <column name="bbnf" length="30">
                <comment>报表年份（和任务的年份相对应）</comment>
            </column>
        </property>
        
        <property name="sffztc" type="java.lang.String">
            <column name="sffztc" length="1">
                <comment>是否复制填充</comment>
            </column>
        </property>
        
        <property name="sfbd" type="java.lang.String">
            <column name="sfbd" length="1">
                <comment>是否比对</comment>
            </column>
        </property>
        
        <property name="bdgzpz" type="java.lang.String">
            <column name="bdgzpz">
                <comment>比对规则配置</comment>
            </column>
        </property>
        
        <property name="fztcgzpz" type="java.lang.String">
            <column name="fztcgzpz">
                <comment>复制填充规则配置</comment>
            </column>
        </property>
        
         <property name="sfyx" type="java.lang.String">
            <column name="sfyx" length="1">
                <comment>是否有效</comment>
            </column>
        </property>
        
    </class>
</hibernate-mapping>
