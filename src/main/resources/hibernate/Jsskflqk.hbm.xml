<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.pfw.framework.ywmk.domain.Jsskflqk" table="t_ywmk_Jsskflqk">
		<comment>教师授课分类情况</comment>
		<id name="id" type="java.lang.String">
			<column name="ID" length="32" />
			<generator class="uuid.hex" />
		</id>
		<property name="jgh" type="java.lang.String">
			<column name="jgh" length="50">
				<comment>教工号</comment>
			</column>
		</property>
		<property name="xm" type="java.lang.String">
			<column name="xm" length="50">
				<comment>姓名</comment>
			</column>
		</property>
		<property name="sfsk" type="java.lang.String">
			<column name="sfsk" length="1">
				<comment>是否授课</comment>
			</column>
		</property>
		<many-to-one name="sklb" class="org.pfw.framework.xtgl.domain.Dict">
			<column name="sklb" length="32">
				<comment>授课类别</comment>
			</column>
		</many-to-one>
		<property name="sfszk" type="java.lang.String">
			<column name="sfszk" length="1">
				<comment>是否思政课</comment>
			</column>
		</property>
		<property name="bskyy" type="java.lang.String">
			<column name="bskyy" length="50">
				<comment>不授课原因:进修,病休,其他.当是否授课为否时</comment>
			</column>
		</property>
		<property name="bz" type="java.lang.String">
			<column name="bz" length="50">
				<comment>备注</comment>
			</column>
		</property>
		<property name="sjnf" type="java.lang.String">
			<column name="sjnf" length="10">
				<comment>数据年份</comment>
			</column>
		</property>

	</class>
</hibernate-mapping>